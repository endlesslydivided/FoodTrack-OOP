                                <ColumnDefinition Width="74*" />
                            </Grid.ColumnDefinitions>

                            <TextBlock
                                Grid.ColumnSpan="2"
                                Height="21"
                                Margin="5,0,5,0"
                                VerticalAlignment="Center"
                                FontFamily="Samsung Sharp Sans"
                                Foreground="Black"
                                Text="Id:" />
                            <TextBlock
                                Grid.Column="1"
                                Height="21"
                                Margin="10,0,10,0"
                                VerticalAlignment="Center"
                                FontFamily="Samsung Sharp Sans"
                                Foreground="Black"
                                Text="{Binding IdSelected}" />

                            <TextBlock
                                Grid.Row="1"
                                Grid.ColumnSpan="2"
                                Height="14"
                                Margin="5,0,5,0"
                                VerticalAlignment="Center"
                                FontFamily="Samsung Sharp Sans"
                                FontSize="10"
                                Foreground="Black"
                                Text="IdParam:" />
                            <TextBox
                                Grid.Row="1"
                                Grid.Column="2"
                                Height="26"
                                Margin="5,0,5,0"
                                VerticalAlignment="Center"
                                mah:TextBoxHelper.ClearTextButton="True"
                                mah:TextBoxHelper.Watermark="Id Пользователя"
                                FontFamily="SamsungOne 400"
                                FontSize="10"
                                Style="{DynamicResource MahApps.Styles.TextBox}"
                                Text="{Binding IdAdded}" />

                            <TextBlock
                                Grid.Row="2"
                                Grid.ColumnSpan="2"
                                Height="12"
                                Margin="5,0,5,0"
                                VerticalAlignment="Center"
                                FontFamily="Samsung Sharp Sans"
                                FontSize="9"
                                Foreground="Black"
                                Text="UserWeight:" />

                            <mah:NumericUpDown
                                Grid.Row="2"
                                Grid.Column="2"
                                Height="26"
                                Margin="5,0,5,0"
                                VerticalAlignment="Center"
                                mah:NumericInputMode="Numbers"
                                mah:ParsingNumberStyle="Integer"
                                mah:TextBoxHelper.Watermark="Кг"
                                Maximum="300"
                                Minimum="1"
                                Value="{Binding Calories}" />

                            <StackPanel
                                Grid.Row="2"
                                Grid.Column="6"
                                Grid.ColumnSpan="2"
                                Margin="5,5,5,5"
                                Orientation="Horizontal">
                                <Button
                                    Margin="7,2,7,2"
                                    Content="Сохранить"
                                    FontFamily="SamsungOne 400"
                                    FontSize="7"
                                    FontWeight="Light" />
                                <Button
                                    Margin="7,2,7,2"
                                    Content="Удалить"
                                    FontFamily="SamsungOne 400"
                                    FontSize="7"
                                    FontWeight="Light" />
                            </StackPanel>
                            <TextBlock
                                Grid.Row="1"
                                Grid.Column="3"
                                Height="10"
                                Margin="5,0,5,0"
                                VerticalAlignment="Center"
                                FontFamily="Samsung Sharp Sans"
                                FontSize="8"
                                Foreground="Black"
                                Text="ParamsDate:" />
                            <DatePicker
                                Grid.Row="1"
                                Grid.Column="4"
                                Margin="5"
                                VerticalAlignment="Center"
                                FontFamily="Samsung Sharp Sans"
                                FontWeight="Normal"
                                SelectedDate="{Binding DateToChoose, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                            <TextBlock
                                Grid.Column="3"
                                Height="12"
                                Margin="5"
                                VerticalAlignment="Center"
                                FontFamily="Samsung Sharp Sans"
                                FontSize="9"
                                Foreground="Black"
                                Text="UserHeight:" />
                            <mah:NumericUpDown
                                Grid.Column="4"
                                Margin="5"
                                VerticalAlignment="Center"
                                mah:NumericInputMode="Numbers"
                                mah:ParsingNumberStyle="Integer"
                                mah:TextBoxHelper.Watermark="См"
                                Maximum="300"
                                Minimum="1"
                                Value="{Binding Calories}" />
                        </Grid>
                    </Border>
                </Grid>
            </mah:FlipViewItem>
        </mah:FlipView>

    </Grid>
</UserControl>
